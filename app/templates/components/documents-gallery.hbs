<article class="gallery">
  {{#each documents as |document| }}
    <div class="col-lg-3 col-centered">
      <div class="hovereffect img-box">
        <img class="img-responsive" src={{ document }} alt="">
        <div class="overlay">
          <a class="info" href="#"><i class="material-icons">camera</i>View Photos</a>
        </div>
      </div>
    </div>
  {{/each}}
</article>
