{{#if model}}
{{filter-bar name=model.name}}
<main role="main">
  <div class="container  head">
    <div class="row">
      <div class="col-md-7">
        <div class="profile">
          <img src={{model.avatar}} class="img-rounded"/>
        </div>
        <h1>{{ model.name }}</h1>
        <p class="disabled">{{ model.lab_kind }}</p>
      </div>
      <div class="col-md-5">
        <span class="pull-right">
            <ul class="list-inline">
              <li>
                <button type="button" class="btn btn-default"><span><i class="material-icons">room</i></span>I've been here</button>
              </li>
              <li>
                <button type="button" class="btn btn-action"><span><i class="material-icons">comment</i></span>Join the Community</button>
              </li>
            </ul>

        </span>
      </div>
    </div>
    <hr>

    <article class="info">
      <div class="row">
        <div class="col-md-7">
          <h3>About</h3>
          <div class="description">
            <p>{{ model.description }}</p>
          </div>
        </div>
        <div class="col-md-5 info">
          <span class="pull-right">
            <ul class="list-inline">
              <li>
                <button type="button" class="special" href="#projects"><h1>16</h1><p>Projects</p>
                </button>
              </li>
              <li>
                <button type="button" class="special" href="#users"><h1>178</h1><p>Users</p>
                </button>
              </li>
              <li>
                <button type="button" class="special" href="#machines"><h1>21</h1><p>Events</p>
                </button>
              </li>
            </ul>
          </span>
        </div>
      </div>
    </article>
  </div>

</main>

<article class="detail">
  <div class="container">
    <div class="row">
      <div class="col-md-7">
        <h3>Lab Details</h3>
        <div class="col-sm-4">
          <span class="disabled"><i class="material-icons">build</i></span>
          <h3>Capabilities</h3>
          <ul>
            <li><p>3D Printing</p></li>
            <li><p>CNC Milling</p></li>
            <li><p>Circuit Production</p></li>
            <li><p>Laser Cutting</p></li>
            <li><p>Laser Engraving</p></li>
            <li><p>Precision Milling</p></li>
            <li><p>Vynil Plotter</p></li>
          </ul>
          <a href="#machines"><p>View Machines →</p></a>
        </div>
        <div class="col-sm-4">
          <span class="disabled"><i class="material-icons">schedule</i></span>
          <h3>Opening Times</h3>
          <ul>
            <li><p>Mon <span>:</span> 9am <span> – </span> 7pm</p></li>
            <li><p>Tue <span>:</span> 9am <span> – </span> 7pm</p></li>
            <li><p>Wed <span>:</span> 9am <span> – </span> 7pm</p></li>
            <li><p><span class="disabled">Thu </span>:<span> Closed</span></p></li>
            <li><p>Fri <span>:</span> 9am <span> – </span> 7pm</p></li>
            <li><p>Sat <span>:</span> 9am <span> – </span> 7pm</p></li>
            <li><p><span class="disabled">Sun </span>:<span> Closed</span></p></li>
          </ul>
        </div>
        <div class="col-sm-4">
          <span class="disabled"><i class="material-icons">room</i></span>
          <h3>Contact</h3>
          <ul>
            <li><p>{{ model.location.address_1 }} {{ model.location.city }} {{ model.location.postal_code }}, {{ model.location.country.data.name }}</p></li>
            <li><p>+34 93 3209520</p></li>
            <li><a href="mailto:info@fablabcn.org"><p>info@fablabbcn.org</p></a></li>
            <ul class="list-inline">
              <li><a href="#comment"><i class="material-icons">public</i></a></li>
              <li><a href="#comment"><i class="material-icons">casino</i></a></li>
              <li><a href="#comment"><i class="material-icons">wc</i></a></li>
              <li><a href="#comment"><i class="material-icons">kitchen</i></a></li>
              <li><a href="#comment"><i class="material-icons">pool</i></a></li>
            </ul>
          </ul>
        </div>
      </div>
      <div class="col-md-5">
        {{ lab-map location=model.location}}
      </div>
    </div>
  </div>
</article>
